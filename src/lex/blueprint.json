      "dialogCodeHook": {
        "uri": "arn:aws:lambda:${self:provider.region}:${AWS::AccountId}:function:${self:custom.namespace}-lex-ask-extend",
        "messageVersion": "1.0"
      },
      "fulfillmentActivity": {
        "type": "ReturnIntent"
      }
